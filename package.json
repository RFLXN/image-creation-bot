{
  "name": "img-creation-bot",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clear": "rimraf -g -I ./dist/**/*",
    "compile": "tsc",
    "precompile": "yarpm run lint:fix && yarpm run clear",
    "start:bot": "node ./dist/main.js",
    "prestart:bot": "yarpm run compile",
    "submodule": "git submodule init && git submodule update",
    "override:bat": "copyfiles webui-user.bat stable-diffusion-webui",
    "override:sh": "copyfiles webui-user.sh stable-diffusion-webui",
    "override:webui": "copyfiles webui.py stable-diffusion-webui",
    "override": "yarpm run override:bat && yarpm run override:sh && yarpm run override:webui",
    "start:webui": "node ./run-webui.js",
    "prestart:webui": "yarpm run submodule && yarpm run override",
    "start:test": "node ./dist/test.js",
    "prestart:test": "yarpm run compile"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "camelize": "^1.0.1",
    "discord-api-types": "^0.37.38",
    "discord.js": "^14.9.0",
    "snakeize": "^0.1.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "yarpm": "^1.2.0"
  }
}
